{
  "name": "unexpected-messy",
  "version": "4.5.0",
  "description": "Unexpected plugin for the messy library",
  "main": "lib/unexpectedMessy.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "jshint .",
    "test": "mocha && npm run lint",
    "travis": "npm test && npm run coverage && (<coverage/lcov.info coveralls || true) && npm run generate-site",
    "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot",
    "generate-site": "node ./generate-site.js",
    "update-examples": "node ./generate-site.js --update-examples && npm run generate-site",
    "deploy-site": "deploy-site"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unexpectedjs/unexpected-messy.git"
  },
  "keywords": [
    "HTTP",
    "request",
    "response",
    "assertion",
    "unexpected",
    "test",
    "mocha"
  ],
  "author": "Andreas Lind <andreas@one.com>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/unexpectedjs/unexpected-messy/issues"
  },
  "homepage": "https://github.com/unexpectedjs/unexpected-messy",
  "peerDependencies": {
    "messy": "^6.5.0",
    "magicpen-media": "^1.5.0",
    "unexpected": "^9.2.0"
  },
  "devDependencies": {
    "coveralls": "2.11.1",
    "istanbul": "0.3.0",
    "jshint": "2.5.5",
    "magicpen": "4.11.0",
    "magicpen-media": "1.5.0",
    "messy": "6.5.0",
    "mocha": "2.2.5",
    "unexpected": "9.2.1",
    "unexpected-documentation-site-generator": "2.5.2"
  },
  "dependencies": {
    "minimist": "1.1.1",
    "underscore": "1.7.0"
  }
}
